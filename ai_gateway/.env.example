# Copy to ai_gateway/.env for local development. Keep real keys out of version control.

# OpenAI credentials used for chat, ASR and TTS features.
AI_GATEWAY_OPENAI_API_KEY=
AI_GATEWAY_OPENAI_MODEL=gpt-4o-mini
AI_GATEWAY_OPENAI_ASR_MODEL=gpt-4o-mini-transcribe
AI_GATEWAY_OPENAI_TTS_MODEL=gpt-4o-mini-tts
AI_GATEWAY_OPENAI_TTS_VOICE=alloy

# Backend connection and CORS configuration.
AI_GATEWAY_BACKEND_BASE_URL=http://localhost:8000
AI_GATEWAY_ALLOWED_ORIGINS=http://localhost:3000
